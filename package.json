{
  "name": "curatify",
  "version": "1.0.0",
  "description": "My fabulous Nuxt.js project",
  "author": "Vadim",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon -r dotenv/config --harmony server/index.js -w server -w lib",
    "build": "nuxt build",
    "start": "NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "migrate:new": "node -r dotenv/config migrate.js create ",
    "migrate:drop": "node -r dotenv/config migrate.js drop",
    "migrate": "node -r dotenv/config migrate.js migrate ",
    "migrate:prod": "node migrate.js migrate ",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@nuxtjs/axios": "^5.0.0",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "body-parser": "^1.18.3",
    "bull": "^3.5.2",
    "bulmaswatch": "^0.7.2",
    "camelcase-keys": "^5.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.2.0",
    "easy-peasy": "^1.14.1",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "material-design-icons": "^3.0.1",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.2.1",
    "pg": "^7.8.0",
    "pg-template-tag": "^0.1.2",
    "postgrator": "^3.7.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "snakecase-keys": "^2.1.0",
    "sugar": "^2.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.17.2",
    "xo": "^0.24.0"
  }
}
