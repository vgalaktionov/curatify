{
  "name": "curatify",
  "version": "1.0.0",
  "description": "My fabulous Nuxt.js project",
  "author": "Vadim",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development concurrently 'npm run dev:backend' 'nom run dev:frontend'",
    "dev:backend": "nodemon --watch 'server/**/*' --ignore 'src/**/*.spec.ts' --exec 'ts-node' server/index.ts",
    "dev:frontend": "npx parcel watch client/index.html client/login.html",
    "start": "NODE_ENV=production node ./built/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "migrate:new": "node -r dotenv/config migrate.js create ",
    "migrate:drop": "node -r dotenv/config migrate.js drop",
    "migrate": "node -r dotenv/config migrate.js migrate ",
    "migrate:prod": "node migrate.js migrate ",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "bulmaswatch": "^0.7.2",
    "camelcase-keys": "^5.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "easy-peasy": "^1.14.1",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "pg": "^7.8.0",
    "pg-template-tag": "^0.1.2",
    "postgrator": "^3.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "snakecase-keys": "^2.1.0",
    "sugar": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@types/camelcase-keys": "^4.1.0",
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.1",
    "@types/pg": "^7.4.14",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "ts": "^0.2.2",
    "typescript": "^3.3.4000"
  }
}
