{
  "name": "curatify",
  "version": "1.0.0",
  "description": "My fabulous Nuxt.js project",
  "author": "Vadim",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development concurrently 'npm run dev:backend' 'npm run dev:frontend'",
    "dev:backend": "nodemon -e ts --watch './server/**/*' --watch './lib/**/*' --watch './types/**/*' --watch './util/**/*' --ignore 'src/**/*.spec.ts' --exec 'tsc && node ./built/server/index.js'",
    "dev:frontend2": "npx webpack -w",
    "dev:frontend": "parcel watch ./client/index.html ./client/login.html",
    "start": "NODE_ENV=production node ./built/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "migrate:new": "node -r dotenv/config migrate.js create ",
    "migrate:drop": "node -r dotenv/config migrate.js drop",
    "migrate": "node -r dotenv/config migrate.js migrate ",
    "migrate:prod": "node migrate.js migrate ",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "bulmaswatch": "^0.7.2",
    "camelcase-keys": "^5.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "csstips": "^0.3.0",
    "csx": "^9.0.0",
    "easy-peasy": "^1.14.1",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "pg": "^7.8.0",
    "pg-template-tag": "^0.1.2",
    "postgrator": "^3.7.0",
    "radium": "^0.25.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "remeda": "0.0.11",
    "retry-axios": "^0.5.0",
    "snakecase-keys": "^2.1.0",
    "ts-node": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@types/camelcase-keys": "^4.1.0",
    "@types/cookie-session": "^2.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/pg": "^7.4.14",
    "@types/radium": "^0.24.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-emotion": "^10.0.9",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "style-loader": "^0.23.1",
    "ts": "^0.2.2",
    "ts-loader": "^5.3.3",
